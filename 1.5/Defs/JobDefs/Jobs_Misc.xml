        <carryThingAfterJob>true</carryThingAfterJob>
        <dropThingBeforeJob>false</dropThingBeforeJob>
    </JobDef>


    <WorkGiverDef>
        <defName>PS_TakePrisonerToPawnStorage</defName>
        <label>take prisoners to storage</label>
        <giverClass>PawnStorages.WorkGiver_Warden_TakeToStorage</giverClass>
        <workType>Warden</workType>
        <verb>take to storage</verb>
        <gerund>taking to storage</gerund>
        <priorityInType>90</priorityInType>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
    </WorkGiverDef>

    <JobDef>
        <defName>PS_CaptureAnimalToFarm</defName>
        <driverClass>PawnStorages.Farm.JobDriver_TakeToFarm</driverClass>
        <reportString>storing TargetA.</reportString>
        <alwaysShowWeapon>true</alwaysShowWeapon>
        <makeTargetPrisoner>false</makeTargetPrisoner>
        <casualInterruptible>false</casualInterruptible>
        <carryThingAfterJob>true</carryThingAfterJob>
    </JobDef>

